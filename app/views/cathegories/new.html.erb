<h1>New Cathegory</h1>
<%= form_for :cathegory, url: cathegories_path do |f| %>
   
  <p>
    <%= f.label :cat %><br>
    <%= f.text_field :cat %>
  </p>
   <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
   <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', cathegories_path %>